
config prplmesh 'config'
        option management_mode 'Multi-AP-Agent'
        option operating_mode 'WDS-Repeater'
        option enable '1'
        option onboarding '0'
        option onboarded '0'
        option master '0'
        option gateway '0'
        option rdkb_extensions '0'
        option band_steering '1'
        option client_roaming '1'
        option dfs_reentry '0'
        option passive_mode '1'
        option wired_backhaul '1'
        option operational '0'
        option ssid 'EasyMesh-BH'
        option mode_enabled 'WPA2-Personal'
        option wep_key '123456789a'
        option key_passphrase '12345678'
        option mem_only_psk '0'
        option backhaul_band 'auto'
        option certification_mode '0'
        option backhaul_wire_iface 'wan'
        option stop_on_failure_attempts '10'
        option check_indirect_connectivity_to_controller_enable '0'
        option controller_discovery_timeout_seconds '240'

config wifi-device 'radio0'
        option hostap_iface 'wlan0-ap'
        option sta_iface 'wlan0-sta-bh'
        option dcs_enable '0'
        option dcs_channel_pool '36-48,149-165'
        option dcs_interval_sec '15'
        option dcs_dwell_msec '40'
        option dcs_refresh_sec '86400'

config wifi-device 'radio1'
        option hostap_iface 'wlan1-ap'
        option sta_iface 'wlan1-sta-bh'
        option dcs_enable '0'
        option dcs_channel_pool '1,6,11'
        option dcs_interval_sec '15'
        option dcs_dwell_msec '40'
        option dcs_refresh_sec '86400'

