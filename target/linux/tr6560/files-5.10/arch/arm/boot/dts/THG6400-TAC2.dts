#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/phy/phy.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/interrupt-controller/arm-gic.h>

#include "triductor-tr6560.dtsi" 

/ {
	model = "THG6400-TAC2";
	compatible = "THG6400-TAC2", "triductor,tr6560";

	leds {
		compatible = "gpio-leds";
		wlan_sharegpio = <1>;

		wlan2g {
			label = "tri:green:wlan2g";
			gpios = <&gpio0 24 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wlan5g {
			label = "tri:green:wlan5g";
			gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wps {
			label = "tri:green:wps";
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		power-red {
			label = "tri:red:power";
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
			default-state = "keep";
		};

		power-green {
			label = "tri:green:power";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;
		
		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio0 23 GPIO_ACTIVE_LOW>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
		};
	};

	// 此节点标识单板gpio特性
	board{
		compatible = "tri,board";
		usb_power = <27>; // usb 电源控制vbus管脚
		current_limit = <28>; //gpio限流保护引脚，目前无芯片使用
		
		tr5120;
		pcie1_1;
		reset_pcie0 = <16>;   //wifi 芯片复位控制管脚
		reset_pcie1 = <9>;

		rgmii;               // rgmii模块属性，涉及GPIO复用配置，不使用请屏蔽
        extphy;              // 带rgmii时的外置phy属性，涉及GPIO复用配置，不使用请屏蔽
        tx_delay     = <4>;  //rgmill tx_delay 0.2ns步进
        rx_delay     = <0>; //rgmill rx_delay 0.2ns步进
        extphy_rstn  = <25>;  //外置phy reset 引脚

        i2c;                // TR6560 i2c 只有一组GPIO24，GPIO25
        i2c_enable   = <0>; // 等于1，使能iic 模块,等于0 不使能i2c 模块
        addr_mode    = <0x1>; //0.7bit地址长度  1:10bit地址长度
        baud_rate    = <0x1>; // 0:100k 1:400k 2:3.4M
		
		jtag_sel;            //wifi芯片操作gpio管脚和jtag复用，jtag优先级更高

	};
};

&halport0 {
	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			reg = <0>;
			label = "wan";
			phy-mode = "internal";
			phy-handle = <&phy1>;
		};
		port@1 {
			reg = <1>;
			label = "lan1";
			phy-mode = "internal";
			phy-handle = <&phy2>;
		};
		port@2 {
			reg = <2>;
			label = "lan2";
			phy-mode = "internal";
			phy-handle = <&phy3>;
		};
		port@3 {
			reg = <3>;
			label = "lan3";
			phy-mode = "internal";
			phy-handle = <&phy4>;
		};
	};

};
